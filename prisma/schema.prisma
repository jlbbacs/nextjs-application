// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" 
}

model Post {
  id      Int    @id @default(autoincrement())
  title   string
  content string
}